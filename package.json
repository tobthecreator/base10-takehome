	"scripts": {
		"build": "yarn",
		"build:local": "rm -rf dist && yarn nx run-many --parallel --target=build --projects barracuda-api,barracuda-app",
		"build:api": "nx run barracuda-api:build",
		"build:app": "nx run barracuda-app:build",
		"prisma:generate": "prisma generate && prisma migrate deploy",
		"prisma:generate-dev": "prisma generate && prisma migrate dev",
		"start": "NEXT_PUBLIC_AUTOMATIC_MONGODB_DATABASE=prod nx run-many --parallel --target=serve --projects barracuda-api,barracuda-app",
		"start:app": "next start ./dist/apps/barracuda-app",
		"start:api": "node dist/apps/barracuda-api/main.js",
		"dev:app": "NEXT_PUBLIC_AUTOMATIC_MONGODB_DATABASE=dev nx run barracuda-app:serve",
		"dev:api": "NEXT_PUBLIC_AUTOMATIC_MONGODB_DATABASE=dev nx run barracuda-api:serve",
		"dev": "rm -rf dist && NEXT_PUBLIC_AUTOMATIC_MONGODB_DATABASE=dev nx run-many --parallel --target=serve --projects barracuda-api,barracuda-app",
		"staging:app": "NEXT_PUBLIC_AUTOMATIC_MONGODB_DATABASE=staging nx run barracuda-app:serve",
		"staging:api": "NEXT_PUBLIC_AUTOMATIC_MONGODB_DATABASE=staging nx run barracuda-api:serve",
		"staging": "rm -rf dist && NEXT_PUBLIC_AUTOMATIC_MONGODB_DATABASE=staging nx run-many --parallel --target=serve --projects barracuda-api,barracuda-app",
		"prod:app": "NEXT_PUBLIC_AUTOMATIC_MONGODB_DATABASE=prod nx run barracuda-app:serve",
		"prod:api": "NEXT_PUBLIC_AUTOMATIC_MONGODB_DATABASE=prod nx run barracuda-api:serve",
		"prod": "rm -rf dist && NEXT_PUBLIC_AUTOMATIC_MONGODB_DATABASE=prod nx run-many --parallel --target=serve --projects barracuda-api,barracuda-app",
		"test:api": "NEXT_PUBLIC_AUTOMATIC_MONGODB_DATABASE=test nx test barracuda-api",
		"make-it-pretty": "prettier --write \"./**/*.{js,jsx,json,ts,tsx}\"",
		"runScript": "rm -rf dist && SCRIPT=true yarn run ts-node -P ./apps/barracuda-api/tsconfig.app.json -r tsconfig-paths/register --require dotenv/config -T"
	},
