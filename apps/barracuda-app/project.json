{
	"root": "apps/barracuda-app",
	"sourceRoot": "apps/barracuda-app",
	"projectType": "application",
	"targets": {
		"build": {
			"executor": "@nrwl/next:build",
			"outputs": ["{options.outputPath}"],
			"defaultConfiguration": "production",
			"options": {
				"root": "apps/barracuda-app",
				"outputPath": "dist/apps/barracuda-app"
			},
			"configurations": {
				"production": {}
			},
			"dependsOn": []
		},
		"serve": {
			"executor": "@nrwl/next:server",
			"options": {
				"buildTarget": "barracuda-app:build",
				"dev": true,
				"proxyConfig": "apps/barracuda-app/proxy.conf.json"
			},
			"configurations": {
				"production": {
					"buildTarget": "barracuda-app:build:production",
					"dev": false
				}
			}
		},
		"export": {
			"executor": "@nrwl/next:export",
			"options": {
				"buildTarget": "barracuda-app:build:production"
			}
		},
		"test": {
			"executor": "@nrwl/jest:jest",
			"outputs": ["coverage/apps/barracuda-app"],
			"options": {
				"jestConfig": "apps/barracuda-app/jest.config.js",
				"passWithNoTests": true
			}
		},
		"lint": {
			"executor": "@nrwl/linter:eslint",
			"outputs": ["{options.outputFile}"],
			"options": {
				"lintFilePatterns": ["apps/barracuda-app/**/*.{ts,tsx,js,jsx}"]
			}
		}
	},
	"tags": ["scope:shared"]
}
